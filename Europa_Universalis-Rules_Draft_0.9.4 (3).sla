                <ITEXT CH="Buntownicy Oblegają lub Ruszają się"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Postanowienia Pokojowe"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
