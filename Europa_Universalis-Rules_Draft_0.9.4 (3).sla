        <PAGEOBJECT XPOS="312.031669291339" YPOS="22595.9" OwnPage="25" ItemID="364272254" PTYPE="4" WIDTH="355.053" HEIGHT="280.730407561192" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PCOLOR="Yellow" SHADE="30" PCOLOR2="Black" PLINEART="1" TEXTFLOWMODE="3" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="2.83464566929134" TEXTRA="2.83464566929134" BEXTRA="2.83464566929134" REXTRA="2.83464566929134" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L355.053 0 L355.053 280.73 L0 280.73 L0 0 Z" copath="M0 -4.03281 L355 -3.31023 L354.022 285.346 L-1.3031 285.004 L0 -4.03281 Z" gXpos="-6.33715302456039e-13" gYpos="56.7266141731959" gWidth="355.053" gHeight="340.826421477977" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" SCALEH="96" CH="Przykład"/>
                <ITEXT SCALEH="96" CH=": Kontynuując przykład z Aktywacji Jednostek ze strony "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" SCALEH="96" CH="16"/>
                <ITEXT SCALEH="96" CH=", ›Kastylia nie używa żadnych Kart Bitwy, ale General (Francisco) dodaje jej 2 (pomarańczowe) Kości Kawalerii do standardowych 3 (białych) Kości Piechoty. Wyrzuca "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="f"/>
                <ITEXT SCALEH="96" CH=", "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="g"/>
                <ITEXT SCALEH="96" CH=", "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="h"/>
                <ITEXT SCALEH="96" CH=", "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="i"/>
                <ITEXT SCALEH="96" CH=" oraz puste pole, co daje 2 trafienia Jednostkami Piechoty i 1 trafienie Jednostkami Kawalerii. Obrońcy rzucają tylko standardowymi 3 Kośćmi Piechoty, otrzymując "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="g"/>
                <ITEXT SCALEH="96" CH=", "/>
                <ITEXT CPARENT="EU symbole" SCALEH="96" CH="h"/>
                <ITEXT SCALEH="96" CH=" i puste pole, co daje 1 trafienie, ponieważ NPR trafiają tylko na wynikach Piechoty (w pełni zacienione sylwetki Piechoty)."/>
                <para PARENT="Domyślny kursywa 11"/>
                <ITEXT CH="›Kastylia musi usunąć 1 Jednostkę jako Straty. Wybiera Jednostkę Piechoty, którą odkłada na pole Wyczerpanych "/>
                <ITEXT CPARENT="EU symbole" CH="m"/>
                <ITEXT CH=" na swojej Planszy Gracza. Obaj obrońcy są usuwani z planszy. ›Kastylia, jako Aktywny Gracz, otrzymuje 1"/>
                <ITEXT CPARENT="EU symbole" CH="c"/>
                <ITEXT CH=" za wygraną Bitwę."/>
                <para PARENT="Domyślny kursywa 11"/>
                <ITEXT SCALEH="88" CH="W swojej następnej Turze ›Kastylia może wydać 2"/>
                <ITEXT CPARENT="EU symbole" SCALEH="88" CH="c"/>
                <ITEXT SCALEH="88" CH=" na Oblężenie Prowincji Granada i Gibraltar."/>
                <trail PARENT="Domyślny kursywa 11"/>
            </StoryText>
        </PAGEOBJECT>
