            <StoryText>
                <DefaultStyle NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="5.5 Akcje Militarne"/>
                <para PARENT="podrozdział nagłówek 15 czerw" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Wypowiedzenie Wojny (1"/>
                <ITEXT CPARENT="EU symbole" CH="c"/>
                <ITEXT CH=")"/>
                <para PARENT="pod-podrozdział nagłówek kursywa 12" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Podczas tej Akcji możesz Wypowiedzieć Wojnę dowolnej liczbie Królestw. W ten sposób rozpoczynasz Wojnę z wybranym Królestwem (Królestwami)."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" SCALEH="97" CH="Uwaga"/>
                <ITEXT SCALEH="97" CH=": Sprawdź ograniczenia DoW (str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" SCALEH="97" CH="22"/>
                <ITEXT SCALEH="97" CH="). Zazwyczaj nie możesz Wypowiedzieć Wojny swoim Sojusznikom, nieodkrytym Odległym Królestwom, graczom, którzy Spasowali, członkom HRE, jeśli Cesarz Spasował lub jest twoim Sojusznikiem, Królestwom, z którymi zawarłeś Rozejm, (TO_CHECK) &quot;on the NPR Allies of ineligible PR targets&quot; lub podczas Bezkrólewia."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Zapłać koszt Akcji, a następnie wykonaj kolejno następujące kroki:"/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Wyznacz Królestwo(a), które będą celem twojego DoW i umieść żeton Wojny na Stolicy każdego z nich."/>
                <para PARENT="Lista bez wcięcia numerowana 1,2,3" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="97" CH="Strać 2"/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="s"/>
                <ITEXT SCALEH="97" CH=" za każde obrane za cel Królestwo, dla którego nie masz żadnego CB (patrz str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" SCALEH="97" CH="22"/>
                <ITEXT SCALEH="97" CH=") i 1"/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="s"/>
                <ITEXT SCALEH="97" CH=" za każde wybrane królestwo, z którym masz "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="M"/>
                <ITEXT SCALEH="97" CH=" (chyba że strona "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="L"/>
                <ITEXT SCALEH="97" CH=" jest odkryta)."/>
                <para PARENT="Lista bez wcięcia numerowana 1,2,3" NumerationName="&lt;local block&gt;"/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" SCALEH="100" CH="Wezwania do Broni"/>
                <ITEXT SCALEH="100" CH=" (w podanej kolejności):"/>
                <para PARENT="Lista bez wcięcia numerowana 1,2,3" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="100" CH="Możesz wysyłać Ofensywne CtA, używając Akcji "/>
                <ITEXT CPARENT="Domyślny pogrubiona kursywa 11" SCALEH="100" CH="Wezwanie do Broni"/>
