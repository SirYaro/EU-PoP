                <ITEXT CH="Niepokoje Religijne"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Otrzymaj/Usuń Niepokój"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Rzuć Kością Buntowników "/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="4. Faza Dochodu i Utrzymania"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Redukcja Kosztów"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Zebranie Dochodu minus Koszty"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Korupcja"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Zebranie Siły Monarchii"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Zdobycie Prestiżu"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="5. Faza Porządków"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Aktualizacja i Odświeżenie"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="97" CH="Czyszczenie Plansz  oraz Planszy Statusu"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="93" CH="Odrzuć karty, aby mieć ich maksymalnie 5 na ręce"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." ParagraphEffectOffset="0" ParagraphEffectIndent="0" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Procedura zakończenia Ery"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="87" CH="4.1 Faza 1: Dobieranie Kart"/>
                <para PARENT="podrozdział nagłówek 15 czerw" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="A. Odsłoń Wydarzenia"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="100" CH="Dobierz Karty Wydarzeń zgodnie z wybranym scenariuszem (zwykle równe liczbie graczy plus 1) z wierzchu Talii Wydarzeń i umieść je w zakryte w wyznaczonym miejscu obok Talię Wydarzeń. Następnie odwróćcie 3 pierwsze 3 karty awersem do góry."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="B. Dobierz Karty Akcji"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="100" CH="Następnie w kolejności Tury, każdy z graczy dobiera 3 karty Akcji, w dowolnej kombinacji, z dowolnej z 3 Talii Kart Akcji (Administracji, Dyplomacji lub Militarnej). Gracze mogą dobierać karty pojedynczo i powinni zachować te karty w tajemnicy przed swoimi Przeciwnikami."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="89" CH="C. Zapłać, aby zatrzymać Karty Akcji"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="100" CH="Gracze muszą zapłacić 2 "/>
                <ITEXT CPARENT="EU symbole" SCALEH="100" CH="D"/>
                <ITEXT SCALEH="100" CH=" za każdą dobraną kartę, którą chcą zatrzymać. Wszystkie dobrane karty, za które nie zapłacono, muszą zostać odrzucone."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="D. Wybierz/Wymień Misje"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Podczas tego kroku każdy gracz, który ma na ręce mniej niż 2 Misje, może wybrać nowe Misje (maksymalnie 2 na ręce). Wszyscy gracze mogą również wymienić aktualnie posiadane Misje."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="4.2 Faza 2: Akcje"/>
                <para PARENT="podrozdział nagłówek 15 czerw" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="92" CH="W tej fazie gracze wykonują swoje Tury w kolejności zgodnej z ruchem wskazówek zegara, zaczynając od gracza, który posiada żeton Pierwszego Gracza (zazwyczaj jest to gracz, który pierwszy spasował w poprzedniej Rundzie). Scenariusz także wskazuje kto zostaje początkowym Pierwszym Graczem. "/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Przebieg Tury"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="94" CH="W swojej Turze Aktywny Gracz może zdecydować się na wykonanie Akcji Podstawowej lub zagranie Karty Akcji z ręki. Większość Akcji ma koszt Siły Monarchii, który musi być zapłacony za pomocą "/>
                <ITEXT CPARENT="EU symbole" SCALEH="94" CH="d"/>
