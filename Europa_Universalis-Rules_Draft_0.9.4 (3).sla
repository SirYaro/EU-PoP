                <ITEXT CH=" na mapie "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" CH="INDII i ARABII"/>
                <ITEXT CH="."/>
                <para PARENT="Punktowanie bez wcięcia czarne"/>
                <ITEXT CH="Zatoka Hudsona, na mapie "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" CH="AMERYKI"/>
                <ITEXT CH=", zwrócona jest w stronę "/>
