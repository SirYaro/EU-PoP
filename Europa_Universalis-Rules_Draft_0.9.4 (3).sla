                <ITEXT CPARENT="EU symbole" CH="&lt;"/>
                <ITEXT CH=")."/>
                <para/>
                <ITEXT CH="W sercu Europy leży Święte Cesarstwo Rzymskie (HRE) z licznymi państwami członkowskimi. To, co znajduje się po żółtej stronie kropkowanej "/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="granicy HRE"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="EU cyfry okrągłe zielo" CH="F"/>
