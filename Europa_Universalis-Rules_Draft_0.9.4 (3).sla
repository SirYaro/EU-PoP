                <ItemAttribute Name="ToC" Type="none" Value="9.1 Deklaracja Wojny (DoW)" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
                <ItemAttribute Name="ToC" Type="none" Value="9.2 Limity Wojenne" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
