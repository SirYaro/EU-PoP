                <ITEXT CPARENT="EU cyfry okrągłe nieb" CH="I"/>
                <ITEXT CH=", na mapie "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" CH="AFRYKI"/>
                <ITEXT CH=", są zwrócone w stronę Strefy Morskiej "/>
