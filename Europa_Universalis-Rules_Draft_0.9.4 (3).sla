                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="97" CH="Gracze wykonują kolejno po 1 Akcji, aż do momentu, gdy wszyscy gracze Spasują."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="3. Faza Pokoju i Buntów"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Usuń Casus Belli i Rozejmy"/>
                <para PARENT="Lista bez wcięcia numerowana A,B,C..." NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="Inwazje NPR"/>
