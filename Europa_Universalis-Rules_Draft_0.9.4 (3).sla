            <StoryText>
                <DefaultStyle/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="Przykład"/>
                <ITEXT CH=": ›Francja (gracz niebieski) chce użyć Akcji "/>
                <ITEXT CPARENT="Domyślny pogrubiona kursywa 11" CH="Wpływy"/>
                <ITEXT CH=", aby zwiększyć swoje wpływy dyplomatyczne we Włoszech. Postanawia umieścić 2 dodatkowe "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" w "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Lombardii"/>
                <ITEXT CH=", gdzie ma już 1"/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=". Oznacza to, że w "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Lombardii"/>
                <ITEXT CH=" znajduje się teraz w sumie 5"/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH="; nie można umieścić tu więcej "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" bez uprzedniego usunięcia żetonów. "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Środkowe Włochy"/>
                <ITEXT CH=" oraz "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Korsyka i Sardynia"/>
                <ITEXT CH=" sąsiadują zarówno z pierwszym ›Francuskim "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" w "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Lombardii"/>
                <ITEXT CH=", jak i z ›Francuskim "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" i "/>
                <ITEXT CPARENT="EU symbole" CH="t"/>
                <ITEXT CH=" w "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Langwedocji"/>
                <ITEXT CH=", więc ›Francja decyduje się umieścić po 1"/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" w każdym z tych Obszarów. ›Francja wydaje 2"/>
                <ITEXT CPARENT="EU symbole" CH="b"/>
                <ITEXT CH=" i 6"/>
                <ITEXT CPARENT="EU symbole" CH="d"/>
                <ITEXT CH=", aby zapłacić za te "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=". Żadne "/>
                <ITEXT CPARENT="EU symbole" CH="I"/>
                <ITEXT CH=" nie może być umieszczone w "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Naples"/>
                <ITEXT CH=" lub "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Dalmacji"/>
                <ITEXT CH=", ponieważ te obszary są poza zasięgiem ›Francji."/>
                <para PARENT="Domyślny kursywa 11"/>
            </StoryText>
        </PAGEOBJECT>
