        <PAGEOBJECT XPOS="128.34662992126" YPOS="35323.9252125985" OwnPage="40" ItemID="157130238" PTYPE="4" WIDTH="538.584" HEIGHT="785.198000000018" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="3" COLGAP="11.3385826771654" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 1.81899e-11 L181.791 1.81899e-11 L538.584 0 L538.584 530.684 L538.584 785.198 L0 785.198 L0 1.81899e-11 Z" copath="M0 1.81899e-11 L538.584 1.81899e-11 L538.584 785.197 L0 785.197 L0 1.81899e-11 Z" gXpos="128.34662992126" gYpos="48.3472598425027" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="157125358"/>
        <PAGEOBJECT XPOS="128.34662992126" YPOS="36205.8146614174" OwnPage="41" ItemID="141367326" PTYPE="4" WIDTH="538.584" HEIGHT="785.198000000018" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="3" COLGAP="11.3385826771654" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 1.81899e-11 L181.791 1.81899e-11 L538.584 0 L538.584 530.684 L538.584 785.198 L0 785.198 L0 1.81899e-11 Z" copath="M0 1.81899e-11 L538.584 1.81899e-11 L538.584 785.197 L0 785.197 L0 1.81899e-11 Z" gXpos="128.34662992126" gYpos="48.3472598425027" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="163376718" BACKITEM="-1">
            <StoryText>
                <DefaultStyle NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="."/>
                <para PARENT="Rozdział linia g"/>
                <ITEXT CH="17. Zwycięstwo i Prestiż ("/>
                <ITEXT CPARENT="EU symbole" FONTSIZE="20" CH="0"/>
                <ITEXT CH=")"/>
                <para PARENT="Rozdział"/>
                <ITEXT CH="."/>
                <para PARENT="Rozdział linia d"/>
                <ITEXT CH="Najczęstszym warunkiem Zwycięstwa w różnych scenariuszach w grze jest posiadanie przez gracza największej liczby Prestiżu ("/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=") na koniec gry."/>
                <para/>
                <ITEXT CH="Prestiż jest zdobywany na kilka różnych sposobów w trakcie i na koniec gry."/>
                <para/>
                <ITEXT CH="Punktacja końcowa odbywa się w Fazie 5 Rundy, w której nastąpił koniec gry, zgodnie z opisem na stronie "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" CH="11"/>
                <ITEXT CH=". Niektóre scenariusze będą miały inne kryteria Punktacji Końcowej."/>
                <para/>
                <ITEXT CH="17.1 Tor Prestiżu"/>
                <para PARENT="podrozdział nagłówek 15 czerw"/>
                <ITEXT CH="Prestiż ("/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=") zdobyty podczas gry jest wskazywany za pomocą znaczników Prestiżu na Torze Prestiżu, który znajduje się na planszy Odległych Kontynentów."/>
                <para/>
                <ITEXT CH="Głównymi źródłami "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=" w trakcie gry są Misje, Kamienie Milowe, Konflikty o Władzę (jeśli są używane), Idee i Wydarzenia."/>
                <para/>
                <ITEXT SCALEH="97" CH="Kiedy "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="0"/>
                <ITEXT SCALEH="97" CH=" gracza przekroczy 60, należy odwrócić jego znacznik Prestiżu na stronę „+60” i kontynuować liczenie dodatkowego "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="0"/>
                <ITEXT SCALEH="97" CH=" od początku toru. Jeśli "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="0"/>
                <ITEXT SCALEH="97" CH=" gracza spadnie poniżej "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="0"/>
                <ITEXT SCALEH="97" CH=", umieść żeton Negatywnego Prestiżu na wierzchu jego znacznika Prestiżu, aby zaznaczyć, że jego pozycja wskazuje ujemną wartość "/>
                <ITEXT CPARENT="EU symbole" SCALEH="97" CH="0"/>
                <ITEXT SCALEH="97" CH="."/>
                <para/>
                <ITEXT CH="W niektórych scenariuszach, koniec gry jest wyzwalany, gdy gracz przekroczy określoną liczbę "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=" na Torze Prestiżu."/>
                <para/>
                <ITEXT CH="17.2 Misje"/>
                <para PARENT="podrozdział nagłówek 15 czerw"/>
                <ITEXT CH="Każde z Głównych Mocarstw w grze posiada własną talię Kart Misji. Dodatkowo istnieją 2 ogólne Talie Misji, które mogą być użyte do tworzenia Talii Misji dla innych Królestw. Każdy Scenariusz określa, na początku gry, jakie Misje są dostępne dla każdego z możliwych do zagrania Królestw."/>
                <para/>
                <ITEXT CH="Niektóre Misje mają zieloną ramkę i tło, co oznacza je jako misje początkowe dla zwykłej rozgrywki 1444. W takiej grze gracze zazwyczaj wybierają 2 z tych Misji na początku gry."/>
                <para/>
                <ITEXT CH="Potrzeba ukończenia Misji zapewnia jasne cele pośrednie w trakcie gry i ma tendencję do kierowania Królestw na wpół-historyczne ścieżki. Są one szczególnie przydatne dla nowych graczy, ale z pewnością dodadzą smaku i dynamiki grze także dla doświadczonych graczy."/>
                <para/>
                <ITEXT CH="Doświadczone grupy, które wolą niczym nie ograniczone rozgrywki, mogą usunąć z gry Misje (jeśli wszyscy gracze się na to zgodzą)."/>
                <para/>
                <ITEXT CH="Wymagania wstępne Misji"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Wszystkie Misje mają Nr Misji (w czerwonym kółku na dole karty). Niektóre Misje wymagają, abyś ukończył "/>
                <ITEXT CPARENT="Domyślny podkresl" CH="przynajmniej jedną"/>
                <ITEXT CH=" z wymienionych wymaganych Misji (wskazanych po lewej stronie Nr Misji), zanim będzie można ją wybrać. Ta z kolei może odblokować inne Misje (wskazane po prawej stronie Nr Misji) po jej ukończeniu."/>
                <para/>
                <ITEXT CH="Ukończenie Misji"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Misje mogą być ukończone w dowolnym momencie, z wyjątkiem Fazy 1. Aby ukończyć Misję, musisz spełnić wymienione w niej wymagania i poinformować o tym pozostałych graczy. W tym momencie natychmiast wchodzą w życie wymienione efekty Misji. Wymagania Misji muszą być spełnione "/>
                <ITEXT CPARENT="Domyślny podkresl" CH="w momencie punktowania"/>
                <ITEXT CH="; wcześniejsze spełnienie wymagań Misji nie liczy się."/>
                <para/>
                <ITEXT CH="Gracz, który ukończy Misję, otrzymuje również liczbę "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=", podaną w prawym górnym rogu karty Misji."/>
                <para/>
                <ITEXT CH="Nagrody za Misje"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Podczas otrzymywania Nagrody za ukończoną Misję obowiązują te same zasady, co w przypadku efektów Standardowych Wydarzeń (patrz str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" CH="40"/>
                <ITEXT CH=")."/>
                <para/>
                <ITEXT CH="Wybieranie nowych Misji"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="W następnej Fazie Dobierania Kart po tym, jak ukończysz i zapunktujesz przynajmniej 1 Misję z ręki, możesz wybrać nową, legalną Misje ze swojej talii tak, abyś ponownie miał 2 Misje na ręce."/>
                <para/>
                <ITEXT CH="W Fazie Dobierania Kart możesz również odłożyć dowolną Misję z ręki z powrotem do talii i wybrać nową, legalną Misję, aby ją zastąpić."/>
                <para/>
                <ITEXT CH="17.3 Kamienie Milowe"/>
                <para PARENT="podrozdział nagłówek 15 czerw"/>
                <ITEXT CH="Kamienie Milowe są podobne do Misji, ale są publiczne i mogą być zapunktowane przez każdego, kto spełni wymagania konieczne do ich zaliczenia. Ilość zdobytego "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=" jest wyższa dla tych, którzy ukończą Kamienie Milowe wcześniej."/>
                <para/>
                <ITEXT CH="Zawsze dostępne są 4 Kamienie Milowe. W każdej Erze losowana jest 1 karta z każdej z 4 talii (Ekspansja, Polityka, Działania Wojenne i Ekonomia). Na koniec każdej Ery należy odrzucić wszystkie Kamienie Milowe i dobrać po jednym z każdej z talii następnej Ery. Scenariusze mogą czasami nakazywać usunięcie pewnych Kamieni Milowych z gry przed jej rozpoczęciem lub wyłożenie predefiniowanych Kamieni Milowych zamiast losowania."/>
                <para/>
                <ITEXT SCALEH="99" CH="Kamienie Milowe mogą być także wymieniane za każdym razem, gdy rozpatrywane jest Wydarzenie „"/>
                <ITEXT CPARENT="Domyślny kursywa 11" SCALEH="99" CH="Dostrzeżono kometę"/>
                <ITEXT SCALEH="99" CH="”. W takim przypadku należy wylosować nowe Kamienie Milowe spośród dostępnych dla bieżącej Ery."/>
                <para/>
                <ITEXT CH="Realizacja Kamienia Milowego"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Pierwszy gracz, który zrealizuje Kamień Milowy, otrzymuje "/>
                <ITEXT CPARENT="EU symbole" CH="5"/>
                <ITEXT CH=", drugi gracz otrzymuje "/>
                <ITEXT CPARENT="EU symbole" CH="3"/>
                <ITEXT CH=", a trzeci "/>
                <ITEXT CPARENT="EU symbole" CH="1"/>
                <ITEXT CH=". Kolejni gracze, którzy zrealizują ten kamień milowy, nie otrzymują "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=". Kiedy zrealizujesz Kamień Milowy, oznacz to żetonem Identyfikacji z twoich zasobów."/>
                <para/>
                <ITEXT CH="Jeśli kilku graczy zrealizuje Kamień Milowy w tym samym czasie, remis rozstrzyga kolejność Tur (zaczynając od Aktywnego Gracza, w Fazie 2, lub od Pierwszego Gracza, w Fazie 3 lub 4), chyba że treść Kamienia Milowego mówi inaczej."/>
                <para/>
                <ITEXT CH="Jak pokazano w prawym dolnym rogu karty Kamienie Milowe zapewniają również nagrody w postaci Siły Monarchii ("/>
                <ITEXT CPARENT="EU symbole" CH="a"/>
                <ITEXT CH=", "/>
                <ITEXT CPARENT="EU symbole" CH="b"/>
                <ITEXT CH=", lub "/>
                <ITEXT CPARENT="EU symbole" CH="c"/>
                <ITEXT CH=") lub Dukatów ("/>
                <ITEXT CPARENT="EU symbole" CH="D"/>
                <ITEXT CH="). Wszyscy gracze, którzy zrealizowali Kamień Milowy, otrzymują tę samą nagrodę."/>
                <para/>
                <ITEXT SCALEH="94" CH="17.4 Konflikty o Władzę"/>
                <para PARENT="podrozdział nagłówek 15 czerw"/>
                <ITEXT CH="Zasady zaawansowane"/>
                <para PARENT="pogrubienie 11 czerwony"/>
                <ITEXT CH="To, czy chcesz włączyć do gry Konflikty o Władzę, czy nie, zależy od twojej grupy graczy. Zalecamy, aby podczas pierwszej rozgrywki grać bez nich."/>
                <para/>
                <ITEXT CH="Konflikty o Władzę zwiększają poziom napięć, interakcji i konflikty pomiędzy PR, ponieważ będą one jednocześnie rywalizować o te same cele."/>
                <para/>
                <ITEXT CH="Tuż pod tytułem karty, nad poziomą linią, każdy Konflikt o Władzę zawiera listę "/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="Obszarów Pól Bitwy"/>
                <ITEXT CH=" ("/>
                <ITEXT CPARENT="EU symbole" CH="§"/>
                <ITEXT CH="), na których gracze będą rywalizować o "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=". Poniżej linii znajdują się dodatkowe kryteria, które pozwolą zdobyć lub stracić "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=", gdy karta będzie punktowana."/>
                <para/>
                <ITEXT CH="Przygotowanie Talii Konfliktów o Władzę"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Scenariusze mają różne zalecane sposoby budowania Talii Konfliktów o Władzę. Niektóre scenariusze sugerują, że Konflikty o Władzę powinny być ułożone w określonej kolejności, podczas gdy inne mogą sugerować kolejność losową."/>
                <para/>
                <ITEXT CH="Talię należy umieścić odkrytą, z najwyższą kartą widoczną dla wszystkich graczy - ta karta jest nazywana "/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="Nadchodzącym Konfliktem o Władzę"/>
                <ITEXT CH="."/>
                <para/>
                <ITEXT CH="Roszczenia Obszarów Pól Bitwy"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Wykonując Akcję "/>
                <ITEXT CPARENT="Domyślny pogrubiona kursywa 11" CH="Sfabrykuj Roszczenia"/>
                <ITEXT CH=" (patrz str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" CH="15"/>
                <ITEXT CH="), wszyscy gracze otrzymują zniżkę w wysokości 1"/>
                <ITEXT CPARENT="EU symbole" CH="b"/>
                <ITEXT CH=" na każde "/>
                <ITEXT CPARENT="EU symbole" CH="G"/>
                <ITEXT CH=" umieszczane na dowolnym Obszarze Pola Bitwy, który jest wymieniony na Aktywnym Konflikcie o Władzę albo Nadchodzącym Konflikcie o Władzę."/>
                <para/>
                <ITEXT SCALEH="98" CH="Aktywowanie Konfliktu o Władzę"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Konflikty o Władzę są aktywowane, gdy zostanie rozstrzygnięty odpowiedni symbol Wydarzenia (patrz punkt 16.5, str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" CH="41"/>
                <ITEXT CH="). Kiedy wierzchnia karta z talii jest przesuwana na miejsce "/>
                <ITEXT CPARENT="Domyślny kursywa 11" CH="Aktywnego Konfliktu o Władzę"/>
                <ITEXT CH=" na Planszy Statusu, ujawnia to także nowy Nadchodzący Konflikt o Władzę, dając graczom szansę na przygotowanie się na to, co nastąpi później."/>
                <para/>
                <ITEXT SCALEH="95" CH="Punktowanie Konfliktów o Władzę"/>
                <para PARENT="pod-podrozdział nagłówek 13"/>
                <ITEXT CH="Każdy "/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="Aktywny Konflikt o Władzę"/>
                <ITEXT CH=" jest punktowany w Fazie Dochodu i Utrzymania, Krok E (patrz str. "/>
                <ITEXT CPARENT="EU cyfry kwadrat i litery" CH="10"/>
                <ITEXT CH="), zanim zostanie odrzucony."/>
                <para/>
                <ITEXT CH="Kiedy Konflikt o Władzę jest punktowany, wszyscy gracze, którzy spełniają warunki, otrzymują "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=", jak opisano poniżej:"/>
                <para/>
                <ITEXT CH="Punktowanie Obszarów Pola Bitwy ("/>
                <ITEXT CPARENT="EU symbole" CH="§"/>
                <ITEXT CH=")"/>
                <para PARENT="Domyślny kursywa pogrubiona 11"/>
                <ITEXT CH="Za każdy Obszar Pola Bitwy gracze otrzymują "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=" w następujący sposób:"/>
                <para/>
                <ITEXT CPARENT="EU symbole" CH="1"/>
                <ITEXT CH=" za każdą Wartość Podatkową Prowincji, które tam Kontrolują ("/>
                <ITEXT CPARENT="EU symbole" CH="v"/>
                <ITEXT CH=" liczy się jako połowa)"/>
                <para PARENT="Lista z wcięciem punktowana •"/>
                <ITEXT CPARENT="EU symbole" CH="1"/>
                <ITEXT CH=" za bycie jedynym PR z "/>
                <ITEXT CPARENT="EU symbole" CH="t"/>
                <ITEXT CH="/"/>
                <ITEXT CPARENT="EU symbole" CH="v"/>
                <ITEXT CH=" w tym miejscu (tylko dla "/>
                <ITEXT CPARENT="EU symbole" CH="§"/>
                <ITEXT CH=" na Mapie Głównej)"/>
                <para PARENT="Lista z wcięciem punktowana •"/>
                <ITEXT CPARENT="EU symbole" CH="1"/>
                <ITEXT CH=" za posiadanie tam przynajmniej 1 "/>
                <ITEXT CPARENT="EU symbole" CH="A"/>
                <ITEXT CH="/"/>
                <ITEXT CPARENT="EU symbole" CH="M"/>
                <para PARENT="Lista z wcięciem punktowana •"/>
                <ITEXT CH="Dodatkowe kryteria Prestiżu"/>
                <para PARENT="Domyślny kursywa pogrubiona 11"/>
                <ITEXT CH="Gracze, którzy spełnią wymienione kryteria, mogą otrzymać dodatkowe "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=", podczas gdy gracze, którzy tego nie zrobią, czasami tracą "/>
                <ITEXT CPARENT="EU symbole" CH="0"/>
                <ITEXT CH=". Te warunki są często specyficzne dla niektórych Królestw."/>
                <para/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="128.34662992126" YPOS="31796.3674173229" OwnPage="36" ItemID="141392670" PTYPE="4" WIDTH="538.584" HEIGHT="785.198000000018" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="3" COLGAP="11.3385826771654" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 1.81899e-11 L181.791 1.81899e-11 L538.584 0 L538.584 530.684 L538.584 785.198 L0 785.198 L0 1.81899e-11 Z" copath="M0 1.81899e-11 L538.584 1.81899e-11 L538.584 785.197 L0 785.197 L0 1.81899e-11 Z" gXpos="128.34662992126" gYpos="48.3472598425027" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="."/>
                <para PARENT="Rozdział linia g" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="14. Buntownicy"/>
                <para PARENT="Rozdział" NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="."/>
                <para PARENT="Rozdział linia d" NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="98" CH="W czasach, w których toczy się akcja gry, Królestwa często miały równie duże problemy z radzeniem sobie z zagrożeniami wewnętrznymi, jak i zewnętrznymi. Bunty były powszechne i stanowią centralny element mechaniki gry."/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT SCALEH="91" CH="Wydarzenia i Niepokoje Religijne powodują, że w PR pojawiają się Niepokoje ("/>
