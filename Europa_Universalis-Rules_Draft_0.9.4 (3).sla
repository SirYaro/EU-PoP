        <PAGEOBJECT XPOS="312.031669291339" YPOS="11513.0896062992" OwnPage="13" ItemID="61221454" PTYPE="4" WIDTH="355.053" HEIGHT="516.410393700804" FRTYPE="3" CLIPEDIT="1" PWIDTH="1" PCOLOR="Yellow" SHADE="30" PCOLOR2="Black" PLINEART="1" TEXTFLOWMODE="3" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="2.83464566929134" TEXTRA="2.83464566929134" BEXTRA="2.83464566929134" REXTRA="5.66929133858268" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L355.053 0 L355.053 516.41 L0 516.41 L0 0 Z" copath="M0 -7.41842 L355 -6.08923 L354.022 524.899 L0 524.379 L0 -7.41842 Z" gXpos="-6.25277607468888e-13" gYpos="0" gWidth="355.053" gHeight="430.410393700802" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="Przykład"/>
                <ITEXT CH=": "/>
                <ITEXT SCALEH="98" CH="›Anglia (czerwony gracz) odkryła Antyle, Kanadę, Pernambuco i Złote Wybrzeże. Ma Statki na "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="98" CH="północno-zachodnim"/>
                <ITEXT SCALEH="98" CH=", "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="98" CH="południowo-wschodnim"/>
                <ITEXT SCALEH="98" CH=" i "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="98" CH="północno-wschodnim Atlantyku"/>
                <ITEXT SCALEH="98" CH=", a teraz chce dokonać Kolonizacji. Posiada 2"/>
                <ITEXT CPARENT="EU symbole" SCALEH="98" CH="&lt;"/>
                <ITEXT SCALEH="98" CH=" w Puli Kolonistów i wydaje 2"/>
                <ITEXT CPARENT="EU symbole" SCALEH="98" CH="a"/>
                <ITEXT SCALEH="98" CH=", aby osiągnąć łączną liczbę 4 wymaganą do ukończenia Akcji Kolonizacji. Może teraz zamienić 1 ze swoich Kolonialnych "/>
                <ITEXT CPARENT="EU symbole" SCALEH="98" CH="G"/>
                <ITEXT SCALEH="98" CH=" na Miasto."/>
                <para PARENT="Domyślny kursywa 11"/>
                <ITEXT CH="Jednakże ›Anglia może przekształcić tylko takie "/>
                <ITEXT CPARENT="EU symbole" CH="G"/>
                <ITEXT CH=", które jest połączone łańcuchem ›angielskich Statków i/lub "/>
                <ITEXT CPARENT="EU symbole" CH="t"/>
                <ITEXT CH=" z jej Obszarem Stołecznym. Na "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Morzu Karaibskim"/>
                <ITEXT CH=" i "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="południowo-zachodnim Atlantyku"/>
                <ITEXT CH=" nie ma żadnego Statku, więc Antyle i Pernambuco "/>
                <ITEXT CPARENT="EU symbole" CH="G"/>
                <ITEXT CH=" nie mogą być zamienione na "/>
                <ITEXT CPARENT="EU symbole" CH="t"/>
                <ITEXT CH=" w tym momencie. Nie można też zamienić "/>
                <ITEXT CPARENT="EU symbole" CH="G"/>
                <ITEXT CH=" na Terytorium Złotego Wybrzeża. Pomimo tego, że na "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="południowo-wschodnim Atlantyku"/>
                <ITEXT CH=" znajduje się Statek, łańcuch ten nie jest kompletny, ponieważ nie ma żadnych Statków w Strefach Morskich pomiędzy "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="południowo-wschodnim Atlantykiem"/>
                <ITEXT CH=" a innymi Strefami Morskimi z ›Angielskimi Statkami."/>
                <para PARENT="Domyślny kursywa 11"/>
                <ITEXT CH="›Anglia może jednak zdecydować się na umieszczenie swojego Miasta w Kanadzie, co pokazują żółte linie. Mając Statek na "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Morzu Karaibskim"/>
                <ITEXT CH=", mogłaby również umieścić miasto na Antylach lub Złotym Wybrzeżu, ponieważ "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="Morze Karaibskie"/>
                <ITEXT CH=" sąsiaduje zarówno z "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="północno-zachodnim"/>
                <ITEXT CH=", jak i "/>
                <ITEXT CPARENT="Domyślny kapitaliki 11" SCALEH="90" CH="południowo-wschodnim Atlantykiem"/>
                <ITEXT CH=" (połączenie "/>
                <ITEXT CPARENT="Domyślny pogrubiona 11" CH="C"/>
                <ITEXT CH=")."/>
                <para PARENT="Domyślny kursywa 11"/>
            </StoryText>
        </PAGEOBJECT>
