                <para PARENT="Rozdział"/>
                <ITEXT CH="."/>
                <para PARENT="Rozdział linia d"/>
                <ITEXT CH="Gra podzielona jest na Ery, z których każda składa się z 4 Rund. Każda Runda podzielona jest na 5 Faz:"/>
                <para NumerationName="&lt;local block&gt;"/>
                <ITEXT CH="1. Faza Dobierania Kart"/>
                <para PARENT="pod-podrozdział nagłówek 13" NumerationName="&lt;local block&gt;"/>
